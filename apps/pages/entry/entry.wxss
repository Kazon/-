page{
  background: url('https://timgsa.baidu.com/timg?image&quality=80&size=b9999_10000&sec=1584448491396&di=e0f28a80db4706548bcd88179b14e920&imgtype=0&src=http%3A%2F%2Fdpic.tiankong.com%2Fhv%2F0v%2FQJ8860255017.jpg');
  display: flex;
  flex-direction: column;
