  align-items: center;
}

text{
  display: block;
  margin: 10rpx 0;
  font-size: 28rpx;
  color: #fff;
}

.flex{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.top{
  width: 340rpx;
  text-align: center;
  margin-top: 20vh;
}

.shop{
  font-size: 38rpx;
  font-weight: 700;
  color: #fff;
}

.cen{
  height: 340rpx;
  width: 340rpx;
}

.userImg{
  width: 180rpx;
  height: 180rpx;
  border: 4rpx solid #fff;
  border-radius: 10rpx;
  background-color: #E8ECEB;
}

.userImg image{
  width: 180rpx;
  height: 180rpx;
  border-radius: 10rpx;
}

.entry{
  display: block;
  padding: 0!important;
  height: 80rpx;
  width: 260rpx!important;
  background-color: rgb(0, 0, 0, 0.3);
  color: #FFFFFF;
  line-height: 72rpx;
  border: 4rpx solid #BFB8B2;
